{
    "name": "andikaryanto11/ci4orm",
    "description": "ORM Database mapping for Codeigniter 4",
    "keywords": ["codeigniter4", "codeigniter", "Eloquent", "ORM","Database"],
    "homepage": "https://packagist.org/packages/andikaryanto11/ciorm",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "fzaninotto/faker": "1.9.2",
        "symfony/yaml": "^5.4"
    },
    "require-dev": {
		"phpunit/phpunit": "^9.5.9",
		"codeception/specify": "^1.4",
		"codeception/verify": "^2.1",
		"phpspec/prophecy-phpunit": "^2.0",
        "mockery/mockery": "^1.5"
	},
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Ci4Orm\\": "src/",
            "Ci4OrmTest\\": "test/"
        }
    }
    
}
